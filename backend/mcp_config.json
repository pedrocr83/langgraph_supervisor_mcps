{
  "mcpServers": {
    "puppeteer": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "brave-search": {
      "transport": "streamable_http",
      "url": "http://localhost:7000/mcp"
    },
    "filesystem": {
      "transport": "stdio",
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "filesystem-mcp-server",
        "sh",
        "-c",
        "exec /rust-mcp-filesystem /workspace"
      ]
    },
    "mcp-db-server": {
      "transport": "stdio",
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "mcp-db-server",
        "python",
        "-m",
        "mcp_server"
      ]
    }
  }
}