{
  "mcpServers": {
    "brave-search": {
      "transport": "streamable_http",
      "url": "http://brave-search-mcp-server:7000/mcp"
    },
    "filesystem": {
      "transport": "stdio",
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "filesystem-mcp-server",
        "sh",
        "-c",
        "exec /rust-mcp-filesystem /workspace"
      ],
      "env": {
        "RUST_LOG": "info"
      }
    },
    "mcp-db-server": {
      "transport": "stdio",
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "mcp-db-server",
        "python",
        "-m",
        "mcp_server"
      ]
    },
    "document-reader": {
      "transport": "stdio",
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "document-reader-mcp-server",
        "python",
        "-m",
        "app.mcp_server"
      ]
    }
  }
}